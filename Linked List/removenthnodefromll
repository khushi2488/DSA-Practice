  #include<bits/stdc++.h>
  using namespace std;
  struct ListNode {
     int val;
     ListNode *next;
     ListNode() : val(0), next(nullptr) {}
    ListNode(int x) : val(x), next(nullptr) {}
  };
  ListNode* removeNthFromEnd(ListNode* head, int n){
    int len=0;
    ListNode* curr=head;
    while(curr!=NULL){
        len++;
        curr=curr->next;
    }
    if(len==n){
       ListNode* temp=head;
       head=head->next;
       delete temp;
       return head;
    }

    int nodetobedeleted=len-n-1;
    curr=head;
    for(int i=0;i<nodetobedeleted;i++){
   curr=curr->next;
    }
    ListNode* temp=curr->next;
    curr->next=curr->next->next;
    delete temp;
    return head;
  }

// tc=O(n)
// sc=O(1)